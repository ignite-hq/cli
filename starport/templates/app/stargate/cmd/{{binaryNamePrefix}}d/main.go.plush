package main

import (
	"os"

    svrcmd "github.com/cosmos/cosmos-sdk/server/cmd"
	"<%= ModulePath %>/app"
	"github.com/tendermint/spm/cosmosapp"
)

func main() {
	rootCmd, _ := cosmosapp.NewRootCmd(
        app.Name,
        app.AccountAddressPrefix,
        app.DefaultNodeHome,
        app.Name,
        app.ModuleBasics,
        app.New,
    )
    if err := svrcmd.Execute(rootCmd, app.DefaultNodeHome); err != nil {
		os.Exit(1)
	}
}
